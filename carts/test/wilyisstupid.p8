mm_flip=false

function mm_draw_standing(x,y)
 -- body
 spr(1,x,y+8,3,2,mm_flip)
 -- head
 spr(33,x+5,y,2,2,mm_flip)
end

function mm_draw_run_1(x,y)
 -- body
 spr(4,x,y+8,2,2,mm_flip)
 -- head
 spr(33,x+7,y+2,2,2,mm_flip)
 -- right arm
 spr(51,x+16,y+10,1,1,mm_flip)
 -- left shoulder
 line(x+5,y+7,x+6,y+7,0)
 -- right toe
 line(x+16,y+22,x+16,y+23,0)
end

function mm_draw_run_2(x,y)
 -- body
 spr(6,x+3,y+8,2,2,mm_flip)
 -- head
 spr(33,x+7,y,2,2,mm_flip)
 -- left shoulder
 pset(x+6,y+7,0)
end

function mm_draw_run_3(x,y)
 -- right arm
 rect(x+16,y+14,x+19,y+15,1)
 line(x+18,y+13,x+19,y+13,0)
 line(x+20,y+14,x+20,y+16,0)
 -- head
 spr(33,x+8,y+2,2,2,mm_flip)
 -- body
 spr(8,x,y+8,2,2,mm_flip)
 -- right foot
 spr(26,x+16,y+16,1,1,mm_flip)
end

function mm_draw_mouth_open(x,y)
 -- head
 spr(33,x,y,2,2,mm_flip)
 -- mouth
 pset(x+5,y+10,15)
 line(x+5,y+11,x+8,y+11,0)
 pset(x+5,y+12,15)
end

function mm_draw_jumping(x,y)
 -- body
 spr(36,x+1,y+13,2,2,mm_flip)
 -- chest
 rectfill(x,y+10,x+11,y+12,12)
 -- left arm
 spr(35,x-7,y+2,1,1,mm_flip)
 line(x-5,y+1,x-4,y+1,0)
 line(x,y+11,x,y+12,0)
 pset(x-1,y+10,0)
 -- right arm
 spr(35,x+11,y+2,1,1,not mm_flip)
 line(x+15,y+1,x+16,y+1,0)
 line(x+11,y+11,x+11,y+12,0)
 pset(x+12,y+10,0)
 -- head
 mm_draw_mouth_open(x,y)
 -- left toe
 line(x+3,y+29,x+5,y+29,0)
end

function _init()
 palt(0,false)
 palt(11,true)
end

function _update()
end

function _draw()
 cls(6)
 mm_draw_standing(8,8)
 mm_draw_run_1(32,8)
 mm_draw_run_2(56,8)
 mm_draw_run_3(80,8)
 mm_draw_mouth_open(104,8)
 mm_draw_jumping(8,40)
end
